###  http://w3id.org/meta-share/omtd-share/AccessComponent
omtd:AccessComponent rdf:type owl:Class ;
                     rdfs:subClassOf omtd:ComponentType ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty omtd:performsOperation ;
                                       owl:someValuesFrom omtd:ResourceAccess
                                     ] ;
                     rdfs:comment "A component that provides access to data resources, e.g. reads a resource or writes the output of a process in a certain format"@en ;
                     rdfs:label "Access Component"@en .


###  http://w3id.org/meta-share/omtd-share/AclAnthologyCorpusFormat
omtd:AclAnthologyCorpusFormat rdf:type owl:Class ;
                              rdfs:subClassOf omtd:CorpusFormat ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty omtd:hasMimetype ;
                                                owl:hasValue omtd:text_plain
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty omtd:documentationUrl ;
                                                owl:hasValue "https://dkpro.github.io/dkpro-core/releases/1.8.0/docs/format-reference.html#format-AclAnthology"
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty omtd:documentationUrl ;
                                                owl:hasValue "https://dkpro.github.io/dkpro-core/releases/1.8.0/docs/format-reference.html#format-de.tudarmstadt.ukp.dkpro.core.io.aclanthology-asl"
                                              ] ;
                              rdfs:comment "Data format specific to the ACL Anthology Reference Corpus (http://acl-arc.comp.nus.edu.sg/), most probably version 20080325"@en ;
                              rdfs:label "ACL Anthology Corpus format"@en .


###  http://w3id.org/meta-share/omtd-share/AgeBracketDetection
omtd:AgeBracketDetection rdf:type owl:Class ;
                         rdfs:subClassOf omtd:InformationExtraction ;
                         rdfs:comment "The task/process of identifying in a text a segment inside parentheses that describes an age group or age-related utterance"@en ;
                         rdfs:label "Age-bracket detection"@en .


###  http://w3id.org/meta-share/omtd-share/AgriculturalEntity
omtd:AgriculturalEntity rdf:type owl:Class ;
                        rdfs:subClassOf omtd:Domain-specificAnnotation ,
                                        omtd:DomainSpecificAnnotationType ;
                        rdfs:comment "Any kind of annotation pertaining to entities of the agricultural domain; the use of the AGROVOC thesaurus is recommended"@en ;
                        rdfs:label "Agricultural entity"@en .


###  http://w3id.org/meta-share/omtd-share/Agriculture
omtd:Agriculture rdf:type owl:Class ;
                 rdfs:subClassOf omtd:Domain ;
                 rdfs:comment "The science, art, or practice of cultivating the soil, producing crops, and raising livestock and in varying degrees the preparation and marketing of the resulting products [https://www.merriam-webster.com/dictionary/agriculture]"@en ;
                 rdfs:label "Agriculture"@en .


###  http://w3id.org/meta-share/omtd-share/AimedCorpusFormat
omtd:AimedCorpusFormat rdf:type owl:Class ;
                       rdfs:subClassOf omtd:CorpusFormat ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty omtd:documentationUrl ;
                                         owl:hasValue "https://builds.openminted.eu/job/WP%205.2%20-%20Typesystem%20alignment/eu.openminted.interop$mapping-conversion/doclinks/1/components.html#Aimed_Collection_Reader"
                                       ] ;
                       rdfs:comment "Format of the Aimed corpus (225 abstracts from MEDLINE) with the gold standard sentence, protein, protein-protein interaction annotations."@en ;
                       rdfs:label "AIMED corpus format"@en .


###  http://w3id.org/meta-share/omtd-share/Aligner
omtd:Aligner rdf:type owl:Class ;
             rdfs:subClassOf omtd:Annotator ;
             rdfs:comment "A component that detects and annotates equivalence relations between items (corpora, texts, paragraphs, sentences, phrases, words) in two languages"@en ;
             rdfs:label "Aligner"@en .


###  http://w3id.org/meta-share/omtd-share/Alignment
omtd:Alignment rdf:type owl:Class ;
               rdfs:subClassOf omtd:MachineTranslationSupport ;
               rdfs:comment "Establishment of translational equivalences between structural units (words, sentences etc.) of a text in a given language and a text with similar meaning in other language(s)"@en ;
               rdfs:label "Alignment"@en .


###  http://w3id.org/meta-share/omtd-share/Alignment1
omtd:Alignment1 rdf:type owl:Class ;
                rdfs:subClassOf omtd:AnnotationType ;
                rdfs:comment "The translational equivalent between structural units (words, sentences etc.) of a text in a given language and a text with similar meaning in other language(s)"@en ;
                rdfs:label "Alignment"@en .


###  http://w3id.org/meta-share/omtd-share/AllbusVariable
omtd:AllbusVariable rdf:type owl:Class ;
                    rdfs:subClassOf omtd:SocialSciencesEntity ;
                    rdfs:comment "ALLBUS variable"@en ;
                    rdfs:label "ALLBUS variable"@en .


###  http://w3id.org/meta-share/omtd-share/Alto
omtd:Alto rdf:type owl:Class ;
          rdfs:subClassOf omtd:Xml ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty omtd:hasMimetype ;
                            owl:hasValue omtd:application_xml_alto
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty omtd:documentationUrl ;
                            owl:hasValue "https://en.wikipedia.org/wiki/ALTO_(XML)"
                          ] ;
          rdfs:label "ALTO" .


###  http://w3id.org/meta-share/omtd-share/AlvisEnrichedDocumentFormat
omtd:AlvisEnrichedDocumentFormat rdf:type owl:Class ;
                                 rdfs:subClassOf omtd:AnnotationFormat ,
                                                 omtd:Xml ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty omtd:hasMimetype ;
                                                   owl:hasValue omtd:application_xml
                                                 ] ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty omtd:documentationUrl ;
                                                   owl:hasValue "http://www.lrec-conf.org/proceedings/lrec2006/pdf/742_pdf.pdf"
                                                 ] ;
                                 rdfs:comment "Format for linguistic annotations of documents used for the ALVIS framework"@en ;
                                 rdfs:label "ALVIS Enriched Document format"@en .


###  http://w3id.org/meta-share/omtd-share/Anafora
omtd:Anafora rdf:type owl:Class ;
             rdfs:subClassOf omtd:AnnotationFormat ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty omtd:hasMimetype ;
                               owl:hasValue omtd:text_x_anafora
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty omtd:documentationUrl ;
                               owl:hasValue "https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5657237/"
                             ] ;
             rdfs:label "Anafora" .


###  http://w3id.org/meta-share/omtd-share/Analyzer
omtd:Analyzer rdf:type owl:Class ;
              rdfs:subClassOf omtd:ComponentType ;
              rdfs:comment "A component that is used for analyzing an input text in order to extract specific features/information (e.g. word list), or to produce statements over the whole text (e.g. classify it by topic)"@en ;
              rdfs:label "Analyzer"@en ;
              skos:altLabel "Extractor"@en .


###  http://w3id.org/meta-share/omtd-share/AnaphoraAnnotation
omtd:AnaphoraAnnotation rdf:type owl:Class ;
                        rdfs:subClassOf omtd:DiscourseAnnotation ;
                        rdfs:comment "The process/task of marking all linguistic expressions that appear in a certain text or across texts as referring to the same entity and linking it to this entity"@en ;
                        rdfs:label "Anaphora annotation"@en ;
                        skos:related omtd:CoReferenceAnnotation .


###  http://w3id.org/meta-share/omtd-share/AnaphoraResolution
omtd:AnaphoraResolution rdf:type owl:Class ;
                        rdfs:subClassOf omtd:LinguisticAnalysis ;
                        rdfs:comment "The process/task of identifying all linguistic expressions that appear in a certain text or across texts and refer to the same entity"@en ;
                        rdfs:label "Anaphora resolution"@en ;
                        skos:related omtd:CoReferenceResolution .


###  http://w3id.org/meta-share/omtd-share/Annotation
omtd:Annotation rdf:type owl:Class ;
                rdfs:subClassOf omtd:Operation ;
                rdfs:comment "A note by way of explanation or comment added to a text or diagram [OED, https://en.oxforddictionaries.com/definition/annotation]. Text or corpus annotation refers to the interpretative linguistic information grounded in a knowledge resource that is added manually or automatically to a text or corpus respectively."@en ,
                             "The process/task of adding annotations to an item"@en ;
                rdfs:label "Annotation"@en ;
                skos:altLabel "Labeling [en_us]"@en_us ,
                              "Labelling"@en ,
                              "Tagging"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationFormat
omtd:AnnotationFormat rdf:type owl:Class ;
                      rdfs:subClassOf omtd:DataFormat ;
                      rdfs:comment "Any format used for annotated files"@en ;
                      rdfs:label "Annotation format"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfCompoundingFeatures
omtd:AnnotationOfCompoundingFeatures rdf:type owl:Class ;
                                     rdfs:subClassOf omtd:MorphologicalAnnotation ;
                                     rdfs:comment "The task/process of marking compounds (single words composed of two or more free morphemes) and their parts"@en ;
                                     rdfs:label "Annotation of compounds"@en ;
                                     owl:deprecated "true"^^xsd:boolean .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfCompounds
omtd:AnnotationOfCompounds rdf:type owl:Class ;
                           rdfs:subClassOf omtd:MorphologicalAnnotation ;
                           dct:replaces omtd:AnnotationOfCompoundingFeatures ;
                           rdfs:comment "The task/process of marking compounds (single words composed of two or more free morphemes) and their parts"@en ;
                           rdfs:label "Annotation of compounds"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfDates
omtd:AnnotationOfDates rdf:type owl:Class ;
                       rdfs:subClassOf omtd:TemporalExpressionsLabelling ;
                       rdfs:comment "The task/process of marking dates in a text"@en ;
                       rdfs:label "Annotation of dates"@en ;
                       skos:altLabel "Date Detection"@en ,
                                     "Date Recognition"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfDerivationalFeatures
omtd:AnnotationOfDerivationalFeatures rdf:type owl:Class ;
                                      rdfs:subClassOf omtd:MorphologicalAnnotation ;
                                      rdfs:comment "The task/process of adding annotations relevant to the derivational level of analysis (e.g. recognizing derivational affixes, tagging their meaning etc.)"@en ;
                                      rdfs:label "Annotation of derivational features"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfDocumentStructure
omtd:AnnotationOfDocumentStructure rdf:type owl:Class ;
                                   rdfs:subClassOf omtd:StructuralAnnotation ;
                                   rdfs:comment "The task/process of annotating the internal structure of a document (e.g. book chapters, sections in a journal article, title, preface, images/figures etc.)"@en ;
                                   rdfs:label "Annotation of document structure"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfMeasurements
omtd:AnnotationOfMeasurements rdf:type owl:Class ;
                              rdfs:subClassOf omtd:SemanticAnnotation ;
                              rdfs:comment "The task/process of annotating measurement expressions (e.g. of length, temperature, time, etc.)"@en ;
                              rdfs:label "Annotation of measurements"@en ;
                              skos:altLabel "Quantity detection"@en ,
                                            "Quantity extraction"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfMultiWordUnits
omtd:AnnotationOfMultiWordUnits rdf:type owl:Class ;
                                rdfs:subClassOf omtd:MorphologicalAnnotation ;
                                rdfs:comment "The task/process of annotating multi-word units, i.e. combinations of words that are considered as one"@en ;
                                rdfs:label "Annotation of multi-word units"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfNumbers
omtd:AnnotationOfNumbers rdf:type owl:Class ;
                         rdfs:subClassOf omtd:SemanticAnnotation ;
                         rdfs:comment "The task/process of annotating numbers in a text"@en ;
                         rdfs:label "Annotation of numbers"@en ;
                         skos:altLabel "Number detection"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfQuestionTopics
omtd:AnnotationOfQuestionTopics rdf:type owl:Class ;
                                rdfs:subClassOf omtd:SemanticAnnotation ;
                                rdfs:comment "The task/process of attaching tags to the segment of a question that functions as the topic of that question"@en ;
                                rdfs:label "Annotation of question topics"@en ;
                                skos:altLabel "Annotation of question topical targets"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfSpeakerTurns
omtd:AnnotationOfSpeakerTurns rdf:type owl:Class ;
                              rdfs:subClassOf omtd:SpeechAnnotation ;
                              rdfs:comment "The task/process of marking locations in speech where there is a change of speakers"@en ;
                              rdfs:label "Annotation of speaker turns"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationOfTextualEntailment
omtd:AnnotationOfTextualEntailment rdf:type owl:Class ;
                                   rdfs:subClassOf omtd:SemanticAnnotation ;
                                   rdfs:comment "The task/process of annotating textual entailments, most usually indicating whether a text entails or contradicts a hypothesis sentence or does neither."@en ;
                                   rdfs:label "Annotation of textual entailment"@en .


###  http://w3id.org/meta-share/omtd-share/AnnotationType
omtd:AnnotationType rdf:type owl:Class ;
