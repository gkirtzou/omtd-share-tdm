#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/meta-share/omtd-share/deployedInDomain
omtd:deployedInDomain rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain omtd:Operation ;
                      rdfs:range omtd:Domain ;
                      rdfs:comment "Concept A is used frequently in domain B"@en ;
                      rdfs:label "deployed in domain"@en .


###  http://w3id.org/meta-share/omtd-share/hasAnnotationType
omtd:hasAnnotationType rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain omtd:Annotation ;
                       rdfs:range omtd:AnnotationType ;
                       rdfs:comment "Used to classify annotations"@en ;
                       rdfs:label "has annotation type"@en .


###  http://w3id.org/meta-share/omtd-share/hasDataFormat
omtd:hasDataFormat rdf:type owl:ObjectProperty ;
                   rdfs:range omtd:DataFormat ;
                   rdfs:label "data format"@en .


###  http://w3id.org/meta-share/omtd-share/hasMethod
omtd:hasMethod rdf:type owl:ObjectProperty ;
               rdfs:range omtd:TdmMethod ;
               rdfs:label "method"@en .


###  http://w3id.org/meta-share/omtd-share/hasMimetype
omtd:hasMimetype rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain omtd:DataFormat ;
                 rdfs:range omtd:Mimetype ;
                 rdfs:comment "Relates a data format to the IANA mimetype; it can be the exact or a broader mimetype; unofficial mimetypes are also used, but this relation will be revisited"@en ;
                 rdfs:label "has mimetype"@en .


###  http://w3id.org/meta-share/omtd-share/performsOperation
omtd:performsOperation rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain omtd:ComponentType ;
                       rdfs:range omtd:Operation ;
                       rdfs:comment "Component A performs Operation B"@en ;
                       rdfs:label "performs Operation"@en ;
                       skos:altLabel "performs Task"@en .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdf:type owl:ObjectProperty .


###  http://www.w3.org/2002/07/owl#equivalentClass
owl:equivalentClass rdf:type owl:ObjectProperty ;
                    rdfs:label "owl:equivalentClass" .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:ObjectProperty .


###  http://www.w3.org/2004/02/skos/core#relatedTo
skos:relatedTo rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://w3id.org/meta-share/omtd-share/documentationUrl
omtd:documentationUrl rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:domain owl:Thing ;
                      rdfs:range xsd:anyURI ;
                      rdfs:comment "The URL link in which a concept is documented"@en ;
                      rdfs:label "documentation URL"@en .


###  http://w3id.org/meta-share/omtd-share/hasFileExtension
omtd:hasFileExtension rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:domain omtd:DataFormat ;
                      rdfs:range xsd:string ;
                      rdfs:comment "The file extension usually associated with a specific data format (e.g. txt for plain text files, pdf for PDF files etc.)"@en ;
                      rdfs:label "has file extension"@en .


###  http://www.w3.org/2004/02/skos/core#broadMatch
skos:broadMatch rdf:type owl:DatatypeProperty .


