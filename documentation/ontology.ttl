                                  rdfs:label "Natural Language Understanding"@en ;
                                  skos:altLabel "LU"@en ,
                                                "Language Understanding"@en ,
                                                "Machine Understanding"@en ,
                                                "NLU"@en ,
                                                "Natural Language Understanding"@en ;
                                  skos:definition "" .


